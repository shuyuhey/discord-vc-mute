{
  "name": "discord-vc-mute",
  "description": "Tool for all user mute in Discord voice channel",
  "homepage": "https://github.com/shuyuhey/discord-vc-mute",
  "repository": "https://github.com/shuyuhey/discord-vc-mute",
  "main": "build/electron/index.js",
  "version": "0.1.1",
  "build": {
    "appId": "me.shuyuhey.app.discord-vc-mute",
    "files": [
      "build",
      "package.json",
      "yarn.lock"
    ],
    "publish": {
      "provider": "github",
      "releaseType": "release"
    }
  },
  "scripts": {
    "dev": "webpack-cli serve --mode development --config webpack.development.config.js",
    "electron:dev": "concurrently \"yarn dev\" \"wait-on http://localhost:3035 && tsc -p electron -w\" \"wait-on http://localhost:3035 && tsc -p electron && electron .\"",
    "build:assets": "rm -rf build/* && webpack --config webpack.production.config.js && tsc -p electron",
    "pack": "electron-builder --publish onTag"
  },
  "dependencies": {
    "@discordjs/opus": "^0.4.0",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.2",
    "@emotion/styled": "^11.0.0",
    "discord.js": "^12.5.1",
    "electron-reload": "^1.5.0",
    "electron-store": "^7.0.1",
    "formik": "^2.2.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@emotion/types": "^0.7.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/ws": "^7.4.0",
    "concurrently": "^5.3.0",
    "electron": "^11.0.3",
    "electron-builder": "^22.9.1",
    "html-webpack-plugin": "^5.1.0",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "wait-on": "^5.2.0",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.7.3"
  }
}
